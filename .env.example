# Node
NODE_ENV=development # Default development
PORT=8080 # Default port
LOGGING=true # Logging app flag
LOGGING_LEVEL=error # Pino minimum log level

# Frontend
FRONTEND_URL=http://localhost:5173 # Frontend url, currently not used

# AWS
AWS_REGION=us-east-1 # AWS region where your resources are located
AWS_ACCESS_KEY_ID=<access key> # User access key
AWS_SECRET_ACCESS_KEY=<secret key> # Secret access key

# Domain
BACKEND_DOMAIN=<backend domain> # 'localhost' for local development

# Cognito
AWS_COGNITO_USER_POOL_ID=<cognito user pool id> # Cognito pool id used for users
AWS_COGNITO_APP_CLIENT_ID=<cognito app client id> # Cognito app client id used for connect cognito with this app

# Cloudflare R2 storage
R2_REGION=auto
R2_ACCOUNT_ID=abc123def456
R2_ACCESS_KEY_ID=1234567890abcdef
R2_SECRET_ACCESS_KEY=abcdef1234567890
R2_BUCKET_NAME=my-app-storage
R2_BUCKET_URL=https://assets.myapp.com

# SES
AWS_SES_EMAIL_IDENTITY=<ses email identity>

# EMAIL
OPS_EMAIL=<ops email>

# Resend
RESEND_API_KEY=<resend api key>

# Marketing
EMAIL_SECRET=<marketing trigger secret>
MARKETING_UNSUBSCRIBE_SECRET=<marketing unsubscribe secret>
MARKETING_QUEUE_NAME=marketing-email
MARKETING_SEND_MAX_PER_RUN=10000

# PROCESS VIDEO SERVER
PROCESS_VIDEO_SERVER_URL=<process video url>

# HEROKU API URL
HEROKU_API_URL=<heroku api url>
VIDEO_SERVICE_APP_ID_OR_NAME=<video service app id or name>

# REDIS URL
REDISCLOUD_URL=<redis cloud url>
REDIS_HOST=<redis host>
REDIS_PORT=<redis port>
REDIS_PASSWORD=<redis password>

#TYPEORM
TYPEORM_CONNECTION=postgres # type of postgres connection
TYPEORM_PORT=5432 # Database host port.
TYPEORM_DATABASE=<database name> #  Database name.
TYPEORM_USERNAME=<database user> # Database username.
TYPEORM_PASSWORD=<database user password> # Database password.
TYPEORM_DRIVER_EXTRA={ "ssl": false } # Typeorm extra driver object.
TYPEORM_ENTITIES=src/entities/**/*.ts # Entities to be loaded for this connection.
TYPEORM_HOST=localhost # Database host.
TYPEORM_LOGGING=false # flag for logging
TYPEORM_MIGRATIONS=src/migrations/**/*.ts # Migrations to be loaded for this connection.
TYPEORM_MIGRATIONS_RUN=false # Indicates if migrations should be auto run on every application launch.
TYPEORM_SYNCHRONIZE=false # Indicates if database schema should be auto created on every application launch.
TYPEORM_SSL={ "rejectUnauthorized": false }

# API_KEYS
API_KEYS=[{ "userId": 1, "apiKey": "API_KEY" }]

# VIDEO_INGESTION_API_KEY
VIDEO_INGESTION_API_KEY=<video ingestion apikey>

# SESSION_SECRET
SESSION_SECRET=<session secret>

# CIPHER_KEY
CIPHER_KEY=<cipher key (32 chars)>

# HEROKU_APIKEY
HEROKU_APIKEY=<heroku apikey>

# WORKOS
WORKOS_CLIENT_ID=<workos client id>
WORKOS_API_KEY=<workos apikey>
WORKOS_CALLBACK_URL=<workos callback url>
WORKOS_COOKIE_PASSWORD=<encrytion string for cookies>
WORKOS_AUTH_URL=<url to authkit frontend url>
WORKOS_ORGANIZATION_ID=<workos organization id>
WORKOS_WEBHOOK_SECRET=<workos webhook secret>

# Google Analytics
GA_MEASUREMENT_ID=<google analytics measurement id>
GA_API_SECRET=<google analytics api secret>

# Designed Playlist UUID
DESIGNED_PLAYLIST_UUID=<designed playlist uuid>

# Routes to log, can use 'all' or 'none' to log, separate routes with a comma
LOG_ROUTES=<routes to log comma separated>

# PRESIGN SERVICE

PRESIGN_SERVICE_URL=< presign url >
PRESIGN_SERVICE_API_KEY=< api key for service >
