{
  "name": "e-dream-ai-backend",
  "version": "0.0.1",
  "description": "Backend of e-dream.ai",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "NODE_PATH=dist node dist/index.js",
    "build": "rm -rf dist & tsc --build",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "dev": "tsx watch src/index.ts",
    "dev-legacy": "nodemon -r dotenv/config --watch 'src/' --exec ts-node-esm -r tsconfig-paths/register src/index.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:unit": "jest --detectOpenHandles --testPathIgnorePatterns='\\.integration\\.test\\.ts$'",
    "test:integration": "jest --detectOpenHandles --testPathPatterns='\\.integration\\.test\\.ts$'",
    "test:unit:ci": "jest --runInBand --testPathIgnorePatterns='\\.integration\\.test\\.ts$'",
    "test:integration:ci": "jest --runInBand --testPathPatterns='\\.integration\\.test\\.ts$'",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "typecheck": "tsc --noEmit",
    "create-env-file": "cp .env.example .env",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm-legacy": "typeorm-ts-node-esm",
    "entity:create": "pnpm run typeorm entity:create src/entities/$entity_name",
    "migration:create": "pnpm run typeorm migration:create src/migrations/$migration_name",
    "migration:generate": "pnpm run typeorm migration:generate src/migrations/$migration_name -d src/database/app-data-source.ts",
    "migration:run": "pnpm run typeorm migration:run -d src/database/app-data-source.ts",
    "migration:revert": "pnpm run typeorm migration:revert -d src/database/app-data-source.ts",
    "migration:show": "pnpm run typeorm migration:show -d src/database/app-data-source.ts",
    "script": "ts-node -r tsconfig-paths/register src/script/$script",
    "migrate:s3-to-r2": "ts-node -r tsconfig-paths/register src/script/migrate-s3-to-r2.ts",
    "transfer:ownership": "ts-node -r tsconfig-paths/register src/script/transfer-ownership.ts",
    "extract-object-keys": "ts-node -r tsconfig-paths/register src/script/extract-object-keys.ts",
    "fix-image-content-types": "ts-node -r tsconfig-paths/register src/script/fix-image-content-types.ts",
    "cron:default-playlist": "NODE_PATH=dist node dist/script/default-playlist.js",
    "husky-install": "husky install",
    "lint-staged": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-dream-ai/backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/e-dream-ai/backend/issues"
  },
  "homepage": "https://github.com/e-dream-ai/backend#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.478.0",
    "@aws-sdk/client-s3": "^3.498.0",
    "@aws-sdk/client-ses": "^3.588.0",
    "@aws-sdk/client-sqs": "^3.485.0",
    "@aws-sdk/s3-presigned-post": "^3.499.0",
    "@aws-sdk/s3-request-presigner": "^3.498.0",
    "@bugsnag/js": "^7.22.4",
    "@bugsnag/plugin-express": "^7.19.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@workos-inc/node": "^7.21.0",
    "aws-sdk": "^2.1456.0",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.1.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "http-status": "^1.7.0",
    "ioredis": "^5.8.1",
    "jest": "^30.2.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "passport": "^0.7.0",
    "passport-cognito": "^0.2.2",
    "passport-headerapikey": "^1.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "pino": "^8.15.0",
    "pino-http": "^10.2.0",
    "pino-pretty": "^10.2.0",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "utility-types": "^3.10.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.496.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.8",
    "@types/node": "^16.18.64",
    "@types/passport": "^1.0.16",
    "@types/passport-cognito": "^0.1.3",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.3.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "socket.io-adapter": "^2.5.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
