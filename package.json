{
  "name": "e-dream-ai-backend",
  "version": "0.0.1",
  "description": "Backend of e-dream.ai",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "build": "rm -rf dist & tsc --build",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "dev": "nodemon -r dotenv/config --watch 'src/' --exec ts-node src/index.ts -e ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-dream-ai/backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/e-dream-ai/backend/issues"
  },
  "homepage": "https://github.com/e-dream-ai/backend#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
