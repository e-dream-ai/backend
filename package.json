{
  "name": "e-dream-ai-backend",
  "version": "0.0.1",
  "description": "Backend of e-dream.ai",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "NODE_PATH=dist node dist/index.js",
    "build": "rm -rf dist & tsc --build",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "dev": "tsx watch src/index.ts",
    "dev-legacy": "nodemon -r dotenv/config --watch 'src/' --exec ts-node-esm -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "create-env-file": "cp .env.example .env",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm-legacy": "typeorm-ts-node-esm",
    "entity:create": "npm run typeorm entity:create -- src/entities/${npm_config_name}",
    "migration:create": "npm run typeorm migration:create -- src/migrations/${npm_config_name}",
    "migration:generate": "npm run typeorm migration:generate -- src/migrations/${npm_config_name} -d src/database/app-data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/database/app-data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/app-data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/database/app-data-source.ts",
    "husky-install": "husky install",
    "lint-staged": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-dream-ai/backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/e-dream-ai/backend/issues"
  },
  "homepage": "https://github.com/e-dream-ai/backend#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.410.0",
    "@aws-sdk/client-s3": "^3.498.0",
    "@aws-sdk/client-sqs": "^3.485.0",
    "@aws-sdk/s3-presigned-post": "^3.499.0",
    "@aws-sdk/s3-request-presigner": "^3.498.0",
    "aws-sdk": "^2.1456.0",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.4.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.496.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.8",
    "@types/node": "^16.18.64",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.3.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
